    DEFPUSHBUTTON   "OK", IDOK, 204, 6, 45, 14, WS_GROUP
    PUSHBUTTON      "Ignore", IDD_CLOSE, 204, 23, 45, 14
    PUSHBUTTON      "&Help", psh15, 204, 40, 45, 14
END

