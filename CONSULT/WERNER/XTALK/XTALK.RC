IDM_MENU MENU
  BEGIN
  POPUP "&File"
    BEGIN
    MENUITEM  "&New phone book entry...",       CMD_NEW
    MENUITEM  "&Open a phone book entry...",    CMD_OPEN
    MENUITEM  "&Save",                          CMD_SAVE
    MENUITEM  "Save &As...",                    CMD_SAVEAS
    MENUITEM  SEPARATOR
    MENUITEM  "Page Se&tup...",                 CMD_PAGESETUP
    MENUITEM  "&Print",                         CMD_PRINT
    MENUITEM  "P&rinter Setup...",              CMD_PRINTSETUP
    MENUITEM  SEPARATOR
    MENUITEM  "&Capture\tF5",                   CMD_CAPTURE
    MENUITEM  "&Upload text...",                CMD_UPLOAD, GRAYED
    MENUITEM  SEPARATOR
    MENUITEM  "S&end with Xmodem...",           CMD_SEND, GRAYED
    MENUITEM  "Rece&ive with Xmodem...",        CMD_RECEIVE, GRAYED
    MENUITEM  SEPARATOR
    MENUITEM  "E&xit",                          CMD_EXIT
    END

  POPUP "&Edit"
    BEGIN
    MENUITEM  "&Copy\tCtrl+Ins",                CMD_COPY
    MENUITEM  "&Paste\tShift+Ins",              CMD_PASTE, GRAYED
    MENUITEM  "Screen to &File...",             CMD_COPYFILE
    MENUITEM  "Screen to C&apture",             CMD_COPYCAPTURE, GRAYED
    MENUITEM  "Screen to &Notes",               CMD_COPYNOTES
    MENUITEM  "Screen to Prin&ter",             CMD_COPYPRINTER
    MENUITEM  "Copy to &Host",                  CMD_COPYSEND, GRAYED
    MENUITEM  "Paste to H&ost",                 CMD_PASTEUPLOAD, GRAYED
    MENUITEM  SEPARATOR
    MENUITEM  "Sc&roll\tScroll-Lock",           CMD_SCROLL
    /*
    MENUITEM  "S&earch Scroll Buffer",          CMD_SEARCHSCROLL
    */
    END

  POPUP "&Actions"
    BEGIN
    MENUITEM  "&Connect\tF6",                   CMD_CONNECT
    MENUITEM  "&Dial...\tF7",                   CMD_DIAL
    MENUITEM  "&Scripts...\tF8",                CMD_EXECUTE
    MENUITEM  "&Notes",                         CMD_NOTES
    MENUITEM  "&Kermit Command Processor",      CMD_KERMIT, GRAYED
    MENUITEM  "&Playback...",                   CMD_PLAYBACK
    MENUITEM  SEPARATOR
    MENUITEM  "&Break\tCtrl+Break",             CMD_BREAK, GRAYED
    MENUITEM  "&Reset Terminal\tCtrl+=",        CMD_RESET
    END

  POPUP "&Setup"
    BEGIN
    MENUITEM  "&Session...",                    CMD_SESSION
    MENUITEM  "&Device (COM1)...",              CMD_COMMUNICATIONS
    MENUITEM  "&Terminal (VT102)...",           CMD_TERMINAL
    MENUITEM  "&Protocol (Xmodem)...",          CMD_PROTOCOL
    MENUITEM  "&ASCII upload settings...",      CMD_ASCII
    MENUITEM  "Function &Keys...",              CMD_MACROS
    MENUITEM  "D&isplay...",                    CMD_DISPLAY
    MENUITEM  "&Fonts...",                      CMD_FONTS
    MENUITEM  SEPARATOR
    MENUITEM  "&Modem...",                      CMD_MODEM
    MENUITEM  "S&ystem...",                     CMD_SYSTEM
    END

  POPUP "&User"
    BEGIN
    MENUITEM  "Empty",                          CMD_MENUNULL
    END

  POPUP "&Window"
    BEGIN
    MENUITEM  "&File Transfer"                  CMD_WINDOW_XFER
    MENUITEM  "&Notes",                         CMD_WINDOW_NOTES
    END

  POPUP "\a&Help"
    BEGIN
    MENUITEM  "Help Topics...",                 CMD_INDEX
    MENUITEM  SEPARATOR
    MENUITEM  "A&bout CROSSTALK...",            CMD_ABOUT
    END
  END

IDM_CISG MENU
  BEGIN
  MENUITEM  "&Copy!",                           CMD_CISG_COPY
  MENUITEM  "&Invert!",                         CMD_CISG_INVERT
  END


